    <add key="UrgentTextFile" value="%USERPROFILE%\Documents\Urgent.txt" />
    <add key="SharedFolder" value="%USERPROFILE%\Documents\SharedFiles" />
    <add key="PublicKey" value="fgsltw" />
    <add key="PublicSalt" value="240 3 45 29 0 76 173 59" />
